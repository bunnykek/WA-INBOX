{
    "name": "WA-INBOX",
    "description": "Gmail inbox client for WhatsApp",
    "repository": "https://github.com/bunnykek/WA-INBOX",
    "logo": "https://imgur.com/TrBVPKT.png",
    "keywords": [
        "whatsapp",
        "gmail",
        "gmail-bot",
        "whatsapp-bot"
    ],
    "env": {
        "client_id": {
            "description": "Get it from credentials.json",
            "value": ""
        },
        "client_secret": {
            "description": "Get it from credentials.json",
            "value": ""
        },
        "refresh_token": {
            "description": "Get it from Generate-Gmail-Token.js",
            "value": ""
        },
        "url": {
            "description": "MongoDB url",
            "value": "mongodb+srv://xxxxxxxxxxxx/myFirstDatabase?retryWrites=true&w=majority"
        },
        "wa_token": {
            "description": "Get it from Generate-WA-Token.js",
            "value": "{\"WABrowserId\":\"\"xxxxxxxx..........}"
        },
        "owner": {
            "description": "The person who wants to receive the email-messages. Ex: 919756xxx890 (including country code but exclude plus sign)",
            "value": "9167xxxxxx89"
        },
        "ignore_links": {
            "description": "true, if you want to ignore agressive hyperlinks. Improves readability.",
            "value": "true"
        }
    },
    "buildpacks": [
        {
          "url": "jontewks/puppeteer"
        },
        {
          "url": "heroku/nodejs"
        }
      ]
}
